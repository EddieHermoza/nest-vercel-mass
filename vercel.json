{
    "version": 2,
    "builds": [
      {
        "src": "src/main.ts",
        "use": "prisma generate && @vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/main.ts",
        "methods": [
          "GET",
          "POST",
          "PUT",
          "PATCH",
          "OPTIONS",
          "DELETE",
          "HEAD",
          "CONNECT",
          "TRACE"
        ]
      }
    ]
  }