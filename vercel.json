{
    "version": 2,
    "builds": [
      {
        "src": "src/main.ts",
        "use": "@vercel/node",
        "config": {
            "includeFiles": ["prisma/schema.prisma", "prisma/**"]
          }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/main.ts",
        "methods": ["GET", "POST", "PUT", "PATCH", "HEAD", "DELETE", "OPTIONS"]
      }
    ],
    "buildCommand": "npm install && npm run prisma:generate && npm run build"
}